first = int(input ("Enter first number- "))
op = input ("Enter operator(+,-,*,/,%) : ")
second = int(input ('Enter second number- '))

if op == "+" :
    print (first + second)
    
elif op == "-" :
    print (first - second)
    
elif op == "*" :
    print (first * second)
    
elif op == "/" :
    print (first / second)
    
elif op == "%" :
    print (first % second)
    
else :
    print ("Invalid operation")
